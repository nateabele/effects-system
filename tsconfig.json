{
  "compilerOptions": {
    "target": "es2017",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "outDir": "./dist",
    "inlineSourceMap": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "typeRoots": [
      "node_modules/@types",
      "./types"
    ]
  }
}